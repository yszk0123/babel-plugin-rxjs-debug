{
  "name": "babel-plugin-rxjs-debug",
  "description": "rxjs debug tools",
  "version": "0.2.0",
  "main": "lib/index.js",
  "repository": "https://github.com/yszk0123/babel-plugin-rxjs-debug.git",
  "author": "Yuji Suzuki <y0suzuki0123@gmail.com>",
  "keywords": [
    "babel-plugin"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "tsc -p tsconfig.build.json",
    "build:production": "run-s clean && NODE_ENV=production tsc -p tsconfig.build.json",
    "fix:format": "prettier --write .",
    "fix:lint": "eslint . --fix",
    "fix": "run-s fix:lint fix:format",
    "test:lint": "eslint .",
    "test:jest": "jest",
    "test:typecheck": "tsc --noEmit",
    "test": "run-p test:*"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.22.20",
    "@types/babel-types": "^7.0.11",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "babel-plugin-tester": "^11.0.4",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.1",
    "tsx": "^3.12.8",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "rxjs": ">=7.0.0"
  }
}
